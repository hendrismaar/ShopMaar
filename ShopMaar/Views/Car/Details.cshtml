@model ShopMaar.Models.Car.CarDetailsViewModel
<h1>Details</h1>
<div>
    <h4>Car</h4>
    <hr />
    <dl class="row">
        <div class="form-group-row">
            <label asp-for="Id" class="col-sm-2 col-sm-label"></label>
            @Html.DisplayFor(model => model.Id)
        </div>
        <div class="form-group-row">
            <label asp-for="Brand" class="col-sm-2 col-sm-label"></label>
            @Html.DisplayFor(model => model.Brand)
        </div>
        <div class="form-group-row">
            <label asp-for="Model" class="col-sm-2 col-sm-label"></label>
            @Html.DisplayFor(model => model.Model)
        </div>
        <div class="form-group-row">
            <label asp-for="Price" class="col-sm-2 col-sm-label"></label>
            @Html.DisplayFor(model => model.Price)
        </div>
        <div class="form-group-row">
            <label asp-for="Description" class="col-sm-2 col-sm-label"></label>
            @Html.DisplayFor(model => model.Description)
        </div>
        <div class="form-group-row">
            <label asp-for="GearShiftType" class="col-sm-2 col-sm-label"></label>
            @Html.DisplayFor(model => model.GearShiftType)
        </div>
        <div class="form-group-row">
            <label asp-for="CreatedAt" class="col-sm-2 col-sm-label"></label>
            @Html.DisplayFor(model => model.CreatedAt)
        </div>
        <div class="form-group-row">
            <label asp-for="ModifiedAt" class="col-sm-2 col-sm-label"></label>
            @Html.DisplayFor(model => model.ModifiedAt)
        </div>
        @foreach (var images in Model.FileToApiViewModels)
        {
            <partial name="_Images" model="images" />
        }
    </dl>
    <a asp-action="Index" asp-controller="Car" class="btn btn-primary">Back</a>
</div>